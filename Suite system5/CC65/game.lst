0038AB  3               ; Dig routine, conditional assembly depending on dflag
0038AB  3               ;------------------------------------------------------------------
0038AB  3               .if dflag
0038AB  3               dig:
0038AB  3               	and #3
0038AB  3               	beq digr		; dig right
0038AB  3               	cmp #1
0038AB  3               	beq digl		; dig left
0038AB  3               	cmp #2
0038AB  3               	beq digd		; dig down
0038AB  3               
0038AB  3               ; Dig up.
0038AB  3               
0038AB  3               digu:				; dig up
0038AB  3               	ldy #8
0038AB  3               	lda (z80_ix),y
0038AB  3               	sec
0038AB  3               	sbc #2
0038AB  3               	sta dispy		; set y
0038AB  3               
0038AB  3               	iny
0038AB  3               	lda (z80_ix),y
0038AB  3               	sta dispx		; set x
0038AB  3               	jmp digv
0038AB  3               
0038AB  3               ; Dig down.
0038AB  3               
0038AB  3               digd:
