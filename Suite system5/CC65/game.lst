0038AB  3               ;-------------------------------------------------------
0038AB  3               ; Entry TICKER command
0038AB  3               ;
0038AB  3               ; Entry:
0038AB  3               ;  z80_b = message nr
0038AB  3               ;  z80_c = width
0038AB  3               ;-------------------------------------------------------
0038AB  3               
0038AB  3               iscrly:
0038AB  3               	jsr prescr 		; set up display position.
0038AB  3               
0038AB  3               	lda #<msgdat 		; text messages.
0038AB  3               	sta z80_l
0038AB  3               	lda #>msgdat
0038AB  3               	sta z80_h
0038AB  3               
0038AB  3               	lda z80_c 		; width.
0038AB  3               	sec
0038AB  3               	sbc #1			; subtract one.
0038AB  3               	cmp #32 		; is it between 1 and 32?
0038AB  3               	bcc :+
0038AB  3               	lda #$60
0038AB  3               	jmp iscrl0		; no, disable messages.
0038AB  3               :
0038AB  3               	ldx z80_b		; message number.
0038AB  3               	jsr getwrd 		; find message start.
0038AB  3               
0038AB  3               	lda z80_l		; set initial text position.
0038AB  3               	sta txtini
0038AB  3               	lda z80_h
0038AB  3               	sta txtini+1
0038AB  3               
0038AB  3               	lda #$ad		; code for lda adrr
0038AB  3               iscrl0:
0038AB  3               	sta scrly		; enable/disable scrolling routine.
0038AB  3               
0038AB  3               	jsr prescr 		; set up display position.
0038AB  3               	jsr gprad 		; get print address.
0038AB  3               
0038AB  3               	lda scraddr 		; set text screen address.
0038AB  3               	sta txtscr
0038AB  3               	lda scraddr+1
0038AB  3               	sta txtscr+1
0038AB  3               
0038AB  3               	lda z80_c		; width.
0038AB  3               	sta txtwid		; set width in working storage.
0038AB  3               
0038AB  3               	lda #128 		; start with leftmost bit.
0038AB  3               	sta txtbit
0038AB  3               
0038AB  3               	jmp scrly4
0038AB  3               .endif
