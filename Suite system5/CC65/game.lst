0038AB  3               ;----------------------------------------------------------
0038AB  3               ; Effects code.
0038AB  3               ; Ticker routine is called 25 times per second.
0038AB  3               ;
0038AB  3               ; HL = txtscr = left text screen address
0038AB  3               ; DE = txtscr+txtwid-1 = right text screen address
0038AB  3               ; BC = txtpos = text scroller position
0038AB  3               ;
0038AB  3               ;----------------------------------------------------------
0038AB  3               
0038AB  3               .if sflag
0038AB  3               scrly:
0038AB  3               	rts
0038AB  3               	.word txtscr         	; get left screen address.
0038AB  3               	sta scr_l
0038AB  3               	lda txtscr+1
0038AB  3               	sta scr_l+1
0038AB  3               	sta scr_r+1
0038AB  3               
0038AB  3               	stx xtmp
0038AB  3               
0038AB  3               	clc         		; get right screen address.
0038AB  3               	lda scr_l
0038AB  3               	adc txtwid
0038AB  3               	sta scr_r
0038AB  3               	dec scr_r
0038AB  3               scrly1:
0038AB  3               	ldy txtwid		; set txtwide
0038AB  3               	dey
0038AB  3               	clc
0038AB  3               scrly0:
0038AB  3               	lda (scr_l),y		; scroll 1 line
0038AB  3               	rol a
0038AB  3               	sta (scr_l),y
0038AB  3               	dey
0038AB  3               	bpl scrly0
0038AB  3               
0038AB  3               	clc			; point to next line
0038AB  3               	lda scr_l
0038AB  3               	adc #32
0038AB  3               	sta scr_l
0038AB  3               	bcc scrly1		; repeat 8 times
0038AB  3               
0038AB  3               	lda txtpos 		; get text pointer.
0038AB  3               	sta scr_txt
0038AB  3               	lda txtpos+1
0038AB  3               	sta scr_txt+1
0038AB  3               
0038AB  3               	ldy #0
0038AB  3               	lda (scr_txt),y 		; find character we're displaying.
0038AB  3               	and #127 		; remove end marker bit if applicable.
0038AB  3               	cmp #13			; is it newline?
0038AB  3               	bne scrly5 		; no, it's okay.
0038AB  3               	lda #32			; convert to a space instead.
0038AB  3               scrly5:
0038AB  3               	sta fntaddr		; calculate char address
0038AB  3               	lda #0
0038AB  3               	sta fntaddr+1
0038AB  3               	asl fntaddr  		; multiply char by 8.
0038AB  3               	rol fntaddr+1
0038AB  3               	asl fntaddr
0038AB  3               	rol fntaddr+1
0038AB  3               	asl fntaddr
0038AB  3               	rol fntaddr+1
0038AB  3               	lda fntaddr
0038AB  3               	clc
0038AB  3               	adc #<(FONT-256)
0038AB  3               	sta scrly3+1		; that's the low byte.
0038AB  3               	lda fntaddr+1
0038AB  3               	adc #>(FONT-256)
0038AB  3               	sta scrly3+2		; add displacement.
0038AB  3               
0038AB  3               	ldx #0
0038AB  3               scrly3:
0038AB  3               	lda $3333,x		; get image of char line.
0038AB  3               	and txtbit
0038AB  3               	beq scrly2		; don't plot pixel
0038AB  3               	ldy scrline,x
0038AB  3               	lda (scr_r),y
0038AB  3               	clc
0038AB  3               	ora #1
0038AB  3               	sta (scr_r),y		; plot pixel
0038AB  3               scrly2:
0038AB  3               	inx			; next line of char.
0038AB  3               	cpx #8
0038AB  3               	bne scrly3
0038AB  3               
0038AB  3               	lsr txtbit		; bit of text to display.
0038AB  3               	bcs :+
0038AB  3               	rts
0038AB  3               :
0038AB  3               	ldy #0
0038AB  3               	lda (scr_txt),y 	; what was the character?
0038AB  3               	asl a	  		; end of message?
0038AB  3               	bcs scrly4
0038AB  3               	inc txtpos
0038AB  3               	bne :+
0038AB  3               	inc txtpos+1
0038AB  3               :
0038AB  3               	jmp scrly6 		; not yet - continue.
0038AB  3               scrly4:
0038AB  3               	lda txtini 		; start of scrolling message.
0038AB  3               	sta txtpos
0038AB  3               	lda txtini+1
0038AB  3               	sta txtpos+1
0038AB  3               scrly6:
0038AB  3               	lda #128
0038AB  3               	sta txtbit
0038AB  3               	ldx xtmp
0038AB  3               	rts
0038AB  3               
0038AB  3               scrline:	.byte $00,$20,$40,$60,$80,$a0,$c0,$e0
